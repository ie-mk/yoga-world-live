{
  "name": "code-school",
  "version": "1.0.0",
  "author": "Justinas Simutis",
  "license": "UNLICENSED",
  "engines": {
    "node": "10"
  },
  "main": "firebaseFunctions.js",
  "scripts": {
    "dev": "next src/ -p 8282",
    "build": "next build src/ && npm run predeploy",
    "start": "next start src/",
    "serve": "npm run build && npm run predeploy && firebase emulators:start --only functions,hosting",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions,hosting",
    "predeploy": "cpx \"src/public/**/*.*\" \"./public\" -C",
    "logs": "firebase functions:log",
    "build-api": "rimraf \"dist-api/**\" &&  NODE_ENV=build webpack --config webpack.api.config.js --progress",
    "cypress:open": "./e2e/node_modules/.bin/cypress open -P e2e",
    "cypress:openLocal": "./e2e/node_modules/.bin/cypress open -P e2e --env E2ELOCAL=true"
  },
  "dependencies": {
    "@zeit/next-css": "1.0.1",
    "babel-plugin-styled-components": "1.10.7",
    "firebase": "7.1.0",
    "firebase-admin": "^8.9.0",
    "firebase-functions": "3.3.0",
    "firebaseui": "^4.5.0",
    "formik": "^2.1.4",
    "i18next": "^19.4.4",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.26.0",
    "next": "9.2.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-firebaseui": "4.1.0",
    "react-i18next": "11.4.0",
    "react-redux": "7.2.0",
    "react-router": "3.2.4",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-form": "8.2.6",
    "redux-notifications": "4.0.1",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "seamless-immutable": "7.1.4",
    "styled-components": "5.1.0",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "cpx": "^1.5.0",
    "cypress": "^4.5.0",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "^7.20.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^8.0.3",
    "husky": "^3.0.8",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "redux-devtools-extension": "2.13.8",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintIgnore": [
    "**/*.plugin.js",
    "**/*.min.js"
  ]
}
